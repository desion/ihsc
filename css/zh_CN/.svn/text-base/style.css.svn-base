/**start logo**/
#head{
   height:26px;
   background:#fff;
   width:100%
}
#head_l{
  padding:0px 0 5px 2px;
}
#head_l li{
   margin:0 40px 0 0;
   padding:0px;
   float:left;
   list-style:none;
}
#head_l li a{
    text-decoration:underline;
    line-height:26px;
    color:#08427c;
    font-size:12px;
}
#head_l li a:hover{
     text-decoration: none;
}
#head_r {
     float:right;
     padding:0px 2px 5px 0;
}
#head_r ul{
     padding:0px;
     margin:0;
}
#head_r li{
    margin:0 0px 0 20px;
    padding:0 0 0 15px;
    float:left;
    list-style:none;
    background-image: url(../../images/x_jt.gif);
    background-repeat: no-repeat;
    background-position: 0px 4px;
}

#head_r li a{
    color:#08427c;
    font-size:12px;
    text-decoration:none;
    
}
#head_r li a:hover{text-decoration: underline;}


#logo {
    width: 100%;
    height: 98px;
    background: #e6e6e6 url(../../images/head_bj.png) repeat-x;
}


.gridview .datalist {
    border-collapse:collapse;
    table-layout:fixed;
    border:1px #08427c solid;width:100%;padding:3px 2px 2px 3px;margin:0px auto
}

.gridview .datalist th {
	border-top:3px solid #8da6c0;
	border-bottom:2px solid #8da6c0;
	border-left:1px solid #8da6c0;
	border-right:1px solid #8da6c0;
	color:#08427c;
	text-align:left;
	vertical-align:middle;
	padding:3px 10px 0px;
	height:25px;
	background:#eef2f6;
	 url(../../imagess/tt_bj.gif);
	font-size: 12px;
	font-weight: normal;
}
.gridview .datalist td {
    text-align:left;
    border:1px solid #9cb3cb;
    vertical-align:bottom;
    padding: 0px 10px 2px;
    height:22px;
}

.gridview .datalist td label{
    /*
    white-space: pre-wrap;
    word-break:break-all;

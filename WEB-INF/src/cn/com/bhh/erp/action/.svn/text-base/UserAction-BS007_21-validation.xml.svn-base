<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC 
		"-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
		"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="user.name">
		<field-validator type="requiredstring" short-circuit="true">
			<param name="trim">true</param>
			<message key="BSE00002"></message>
		</field-validator>

        <field-validator type="regex" short-circuit="true">
            <param name="expression" ><![CDATA[(\w)+]]></param>
            <message key="BSE01021"></message>
        </field-validator>
        
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">20</param>
			<param name="trim">true</param>
			<message key="BSE00003"></message>
		</field-validator>
	</field>
	<field name="user.password">
		<field-validator type="requiredstring" short-circuit="true">
			<param name="trim">true</param>
			<message key="BSE00002"></message>
		</field-validator>

        <field-validator type="regex" short-circuit="true">
            <param name="expression" ><![CDATA[[^\u4E00-\u9FA5]+]]></param>
            <message key="BSE01029"></message>
        </field-validator>
        
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">20</param>
			<param name="trim">true</param>
			<message key="BSE00003"></message>
		</field-validator>
	</field>
	
	<field name="user.repeatPassword">
		<field-validator type="requiredstring" short-circuit="true">
			<message key="BSE00002"></message>
		</field-validator>

        <field-validator type="regex" short-circuit="true">
            <param name="expression" ><![CDATA[[^\u4E00-\u9FA5]+]]></param>
            <message key="BSE01029"></message>
        </field-validator>
        
		<field-validator type="fieldexpression">
			<param name="expression">user.repeatPassword==user.password</param>
			<message key="BSE01003"></message>
		</field-validator>

	</field>
	<field name="user.familyName">
		<field-validator type="requiredstring" short-circuit="true">
			<param name="trim">true</param>
			<message key="BSE00002"></message>
		</field-validator>

		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">20</param>
			<param name="trim">true</param>
			<message key="BSE00003"></message>
		</field-validator>
	</field>
	
	<field name="user.givenName">
		<field-validator type="requiredstring" short-circuit="true">
			<param name="trim">true</param>
			<message key="BSE00002"></message>
		</field-validator>	
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">20</param>
			<param name="trim">true</param>
			<message key="BSE00003"></message>
		</field-validator>
	</field>
	

	<field name="user.companyID">
		<field-validator type="required" short-circuit="true">
			<param name="trim">true</param>
			<message key="BSE00002"></message>
		</field-validator>
	</field>

	<field name="user.officePhone">
		<field-validator type="regex">
            <param name="expression" ><![CDATA[(\d|-|\/)*]]></param>
            <message key="BSE00004"></message>
        </field-validator>
        
        <field-validator type="stringlength">
            <param name="minLength">1</param>
            <param name="maxLength">60</param>
            <param name="trim">true</param>
            <message key="BSE00003"></message>
        </field-validator>
	</field>
	
	<field name="user.fax">
		<field-validator type="regex">
            <param name="expression" ><![CDATA[(\d|-|\/)*]]></param>
            <message key="BSE00004"></message>
        </field-validator>
        
        <field-validator type="stringlength">
            <param name="minLength">1</param>
            <param name="maxLength">60</param>
            <param name="trim">true</param>
            <message key="BSE00003"></message>
        </field-validator>
	</field>
	
	
	<field name="user.homePhone">
		<field-validator type="regex">
            <param name="expression" ><![CDATA[(\d|-|\/)*]]></param>
            <message key="BSE00004"></message>
        </field-validator>
        
        <field-validator type="stringlength">
            <param name="minLength">1</param>
            <param name="maxLength">60</param>
            <param name="trim">true</param>
            <message key="BSE00003"></message>
        </field-validator>
	</field>
	
	 <field name="user.mobilePhone">
		<field-validator type="regex">
            <param name="expression" ><![CDATA[(\d|-|\/)*]]></param>
            <message key="BSE00004"></message>
        </field-validator>
        
        <field-validator type="stringlength">
            <param name="minLength">1</param>
            <param name="maxLength">60</param>
            <param name="trim">true</param>
            <message key="BSE00003"></message>
        </field-validator>
	</field>
	
	
    <field name="user.email">
		<field-validator type="email">
			<message key="BSE00004"></message>
		</field-validator>
		
		 <field-validator type="stringlength">
            <param name="minLength">1</param>
            <param name="maxLength">80</param>
            <param name="trim">true</param>
            <message key="BSE00003"></message>
        </field-validator>
	</field>
	
	<field name="user.companyID">
        <field-validator type="required" short-circuit="true">
            <param name="trim">true</param>
            <message key="BSE00002"></message>
        </field-validator>
    </field>
	
	<field name="user.department">
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">40</param>
			<param name="trim">true</param>
			<message key="BSE00003"></message>
		</field-validator>
	</field>
	
	<field name="user.post">
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">40</param>
			<param name="trim">true</param>
			<message key="BSE00003"></message>
		</field-validator>
	</field>
	

	<field name="user.groupId">
		<field-validator type="required" >
			<message key="BSE00002"></message>
		</field-validator>
	</field>
	
	
</validators>